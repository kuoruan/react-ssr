    "build": "npm-run-all --parallel build:client build:server",
    "build:client": "webpack",
    "build:server": "webpack --env server",
    "build:staging": "npm-run-all --parallel build:staging:client build:staging:server",
    "build:staging:client": "webpack --env MODE=staging",
    "build:staging:server": "webpack --env server --env MODE=staging",
    "serve": "webpack --env dev --env server",
    "start": "node ./dist/server/server.js --port 8002",
    "test": "jest",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "lint:style": "stylelint '**/*.{scss,less,css}' --quiet --fix"
